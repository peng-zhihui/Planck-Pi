/dts-v1/;

#include "mt6797.dtsi"

/ {
	model = "Mediatek X20 Development Board";
	compatible = "archermind,mt6797-x20-dev", "mediatek,mt6797";

	aliases {
		serial0 = &uart1;
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0 0x40000000 0 0x80000000>;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_pins_a>;
};
