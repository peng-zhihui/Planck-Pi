if TARGET_KMETER1

config SYS_BOARD
	default "km83xx"

config SYS_VENDOR
	default "keymile"

config SYS_CONFIG_NAME
	default "kmeter1"

config BOARD_SPECIFIC_OPTIONS # dummy
	def_bool y
	select ARCH_MPC8360
	imply CMD_CRAMFS
	imply CMD_DIAG
	imply FS_CRAMFS

endif

