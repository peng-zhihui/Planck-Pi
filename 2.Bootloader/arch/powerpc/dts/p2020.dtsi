/dts-v1/;

/include/ "e500v2_power_isa.dtsi"

/ {
	#address-cells = <2>;
	#size-cells = <2>;
	interrupt-parent = <&mpic>;

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu0: PowerPC,P2020@0 {
			device_type = "cpu";
			reg = <0>;
		};
		cpu1: PowerPC,P2020@1 {
			device_type = "cpu";
			reg = <1>;
		};
	};
};
