#! /bin/sh

set -e

case "$1" in
    remove|disappear)
    if which update-mime >/dev/null;
    then
            update-mime
    fi
    if test -L /usr/bin/which; then
	    rm /usr/bin/which
    fi
        ;;
    upgrade|failed-upgrade|purge|abort-install|abort-upgrade)
        ;;
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac
